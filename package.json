{
  "name": "typo",
  "version": "0.1.0",
  "private": true,
  "main": "dist/main/main.js",
  "scripts": {
    "clean:dist": "rm -rf ./dist",
    "clean:release": "rm -rf ./release",
    "clean:all": "npm run clean:dist & npm run clean:release",
    "serve:vue": "vue-cli-service serve",
    "build:vue": "vue-cli-service build",
    "build:main": "webpack",
    "build:both:dev": "npm run clean:dist && webpack & vue-cli-service build",
    "build:release": "npm run clean:dist && vue-cli-service build && NODE_ENV=production webpack",
    "make:mac": "npm run clean:all && npm run build:release && build --mac --x64",
    "make:win": "echo Not Supported!",
    "lint:vue": "vue-cli-service lint",
    "test:unit:vue": "vue-cli-service test:unit",
    "run": "electron dist/main/main.js",
    "rebuild": "electron-rebuild -w font-manager -p -v 3.0.13",
    "postinstall": "npm run rebuild"
  },
  "dependencies": {
    "buefy": "^0.7.1",
    "electron-store": "^2.0.0",
    "electron-util": "^0.10.2",
    "font-manager": "^0.3.0",
    "vue": "^2.5.17",
    "vue-property-decorator": "^7.2.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-module-decorators": "^0.9.7"
  },
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/electron-store": "^1.3.0",
    "@types/electron-util": "^0.2.0",
    "@types/mocha": "^5.2.4",
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-e2e-cypress": "^3.2.0",
    "@vue/cli-plugin-typescript": "^3.2.0",
    "@vue/cli-plugin-unit-mocha": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/eslint-config-typescript": "^3.2.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-minify-webpack-plugin": "^0.3.1",
    "chai": "^4.1.2",
    "electron": "^3.0.13",
    "electron-builder": "^20.38.3",
    "electron-rebuild": "^1.8.2",
    "eslint": "^5.10.0",
    "eslint-plugin-vue": "^5.0.0",
    "moment": "^2.23.0",
    "node-sass": "^4.9.0",
    "prettier": "^1.15.3",
    "reflect-metadata": "^0.1.12",
    "sass-loader": "^7.0.1",
    "ts-loader": "^5.3.1",
    "typescript": "^3.2.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
